{
    "name": "Spotify Sampled",
    "version": "0.1",
    "description": "Get the sample used in a song on Spotify",
    "permissions": ["storage"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [
        {
          "matches": ["http://*.open.spotify.com//*"],
          "run_at": "document_end",
          "js": ["contentScript.js"]
        }
    ],
    "manifest_version": 2
}